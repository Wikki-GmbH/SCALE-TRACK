FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rhoV;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0.00865;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
        value           uniform 0.00865;
    }
}


