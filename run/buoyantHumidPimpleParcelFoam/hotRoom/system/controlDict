FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     buoyantPimpleJuliaParcelFoam_250321;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         2;

deltaT          0.002;

writeControl    runTime;

writeInterval   30;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   5;

runTimeModifiable true;

